{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "mocha": true
    },
    "rules": {
      "linebreak-style": 0,
      "import/no-extraneous-dependencies": ["error", {"optionalDependencies": false, "peerDependencies": false}],
      "no-unused-vars": 0,
      "no-param-reassign": 0,
      "func-names": 0
    }
}
